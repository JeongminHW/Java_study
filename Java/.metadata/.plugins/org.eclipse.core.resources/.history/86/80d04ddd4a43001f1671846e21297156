package ch04;

import java.util.*;

public class LuckyLotto3 {

	public static void main(String[] args) {
		Object arr[] = getLotto();

		for (int i = 0; i < arr.length; i++) {
			System.out.println(arr[i] + "\t");

		}

	}

	public static Object[] getLotto() {
		int lotto[] = new int[6];
		Random rd = new Random();
		Arrays.sort(lotto); // 자동 정렬(오름차순)
		return lotto;
	}

}
