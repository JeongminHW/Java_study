package ch04;

public class Ex3 {

	public static void main(String[] args) {
		String month = "feburary";
		switch(month) {
		case "january":
			System.out.println("1월");
			break;
		case "feburary":
			System.out.println("2월");
			break;
		case "march":
			System.out.println("3월");
			break;
		case "april":
			System.out.println("4월");
			break;
		default:
			System.out.println("4월 이후");
		}
		
		
	}

}
