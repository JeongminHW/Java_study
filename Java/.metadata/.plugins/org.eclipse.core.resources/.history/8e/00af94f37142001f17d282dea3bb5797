package ch02;

public class Ex3 {

	public static void main(String[] args) {
		// 데이터 타입(Data type) : 기본형 (8개) + 참조형(무한대, 클래스 타입)
		// 정수형(4)
		byte b = 10;
		short s = 10;
		int i = 20; // 기본
		long l = 30;
		
		// 실수형(2)
		float f = 3.14f; // 리터럴값에 뒤에 f를 반드시 표기해줘야 함
		double d = 3.14; // 기본
		
		// 문자형(2)
		char 
		
		// 불린형(1)
	}

}
