package ch04;

import java.util.Scanner;

public class Ex2 {

	public static void main(String[] args) {
		//1~20 사이의 숫자 중에 3, 6, 9에 해당하는 숫자는 짝
		System.out.println("3,6,9 게임 숫자를 입력 : ");
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = a%10; // 십의 자리 날림
		if(b==3 || b==6 || b==9)
			System.out.println("짝");
		else
			System.out.println(a);
	}

}
