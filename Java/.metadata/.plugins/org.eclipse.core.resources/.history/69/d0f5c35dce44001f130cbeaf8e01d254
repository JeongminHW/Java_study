class Fianl3 {
	// 상수 : 한 번 정해진 값의 변경이 불가능 한 변수
	static final int KIA = 1; // 대부분 대문자로 표기
	// Math.PI;
	//Color.red;
	
	
	// 상수는 반드시 초기화 시켜줘야함
	int a;
	final static int LOTTE = 2;
	
	// 상수는 변수명 자체가 의미를 가짐
	Label label = new Label("하하", 1);
}

